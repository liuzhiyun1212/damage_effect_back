<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.project.system.mapper.EquipmentDeploymentData8Mapper">
    
    <resultMap type="EquipmentDeploymentData8" id="EquipmentDeploymentData8Result">
        <result property="id"    column="id"    />
        <result property="planeType"    column="plane_type"    />
        <result property="troops"    column="troops"    />
        <result property="deploymentTime"    column="deployment_time"    />
        <result property="deploymentQuantity"    column="deployment_quantity"    />
    </resultMap>

    <sql id="selectEquipmentDeploymentData8Vo">
        select id, plane_type, troops, deployment_time, deployment_quantity from equipment_deployment_data_8
    </sql>

    <select id="selectEquipmentDeploymentData8List" parameterType="EquipmentDeploymentData8" resultMap="EquipmentDeploymentData8Result">
        <include refid="selectEquipmentDeploymentData8Vo"/>
        <where>  
            <if test="planeType != null  and planeType != ''"> and plane_type = #{planeType}</if>
            <if test="troops != null  and troops != ''"> and troops = #{troops}</if>
            <if test="deploymentTime != null "> and deployment_time = #{deploymentTime}</if>
            <if test="deploymentQuantity != null "> and deployment_quantity = #{deploymentQuantity}</if>
        </where>
    </select>
    
    <select id="selectEquipmentDeploymentData8ById" parameterType="Long" resultMap="EquipmentDeploymentData8Result">
        <include refid="selectEquipmentDeploymentData8Vo"/>
        where id = #{id}
    </select>
        
    <insert id="insertEquipmentDeploymentData8" parameterType="EquipmentDeploymentData8">
        insert into equipment_deployment_data_8
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="planeType != null">plane_type,</if>
            <if test="troops != null">troops,</if>
            <if test="deploymentTime != null">deployment_time,</if>
            <if test="deploymentQuantity != null">deployment_quantity,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="planeType != null">#{planeType},</if>
            <if test="troops != null">#{troops},</if>
            <if test="deploymentTime != null">#{deploymentTime},</if>
            <if test="deploymentQuantity != null">#{deploymentQuantity},</if>
         </trim>
    </insert>

    <update id="updateEquipmentDeploymentData8" parameterType="EquipmentDeploymentData8">
        update equipment_deployment_data_8
        <trim prefix="SET" suffixOverrides=",">
            <if test="planeType != null">plane_type = #{planeType},</if>
            <if test="troops != null">troops = #{troops},</if>
            <if test="deploymentTime != null">deployment_time = #{deploymentTime},</if>
            <if test="deploymentQuantity != null">deployment_quantity = #{deploymentQuantity},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteEquipmentDeploymentData8ById" parameterType="Long">
        delete from equipment_deployment_data_8 where id = #{id}
    </delete>

    <delete id="deleteEquipmentDeploymentData8ByIds" parameterType="String">
        delete from equipment_deployment_data_8 where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>