<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.project.system.mapper.ProductCreateMapper">
    
    <resultMap type="ProductCreate" id="ProductCreateResult">
        <result property="id"    column="id"    />
        <result property="productName"    column="product_name"    />
        <result property="productModel"    column="product_model"    />
        <result property="productManufacturer"    column="product_manufacturer"    />
        <result property="productBatch"    column="product_batch"    />
    </resultMap>

    <sql id="selectProductCreateVo">
        select id, product_name, product_model, product_manufacturer, product_batch from product_create_5
    </sql>

    <select id="selectProductCreateList" parameterType="ProductCreate" resultMap="ProductCreateResult">
        <include refid="selectProductCreateVo"/>
        <where>  
            <if test="productName != null  and productName != ''"> and product_name like concat('%', #{productName}, '%')</if>
            <if test="productModel != null  and productModel != ''"> and product_model = #{productModel}</if>
            <if test="productManufacturer != null  and productManufacturer != ''"> and product_manufacturer = #{productManufacturer}</if>
            <if test="productBatch != null "> and product_batch = #{productBatch}</if>
        </where>
    </select>
    
    <select id="selectProductCreateById" parameterType="Long" resultMap="ProductCreateResult">
        <include refid="selectProductCreateVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertProductCreate" parameterType="ProductCreate" useGeneratedKeys="true" keyProperty="id">
        insert into product_create_5
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="productName != null">product_name,</if>
            <if test="productModel != null">product_model,</if>
            <if test="productManufacturer != null">product_manufacturer,</if>
            <if test="productBatch != null">product_batch,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="productName != null">#{productName},</if>
            <if test="productModel != null">#{productModel},</if>
            <if test="productManufacturer != null">#{productManufacturer},</if>
            <if test="productBatch != null">#{productBatch},</if>
         </trim>
    </insert>

    <update id="updateProductCreate" parameterType="ProductCreate">
        update product_create_5
        <trim prefix="SET" suffixOverrides=",">
            <if test="productName != null">product_name = #{productName},</if>
            <if test="productModel != null">product_model = #{productModel},</if>
            <if test="productManufacturer != null">product_manufacturer = #{productManufacturer},</if>
            <if test="productBatch != null">product_batch = #{productBatch},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteProductCreateById" parameterType="Long">
        delete from product_create_5 where id = #{id}
    </delete>

    <delete id="deleteProductCreateByIds" parameterType="String">
        delete from product_create_5 where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>